<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\Delimiter" name="customer-account-navigation-delimiter-3" template="Magento_Customer::account/navigation-delimiter.phtml">
                <arguments>
                    <argument name="sortOrder" xsi:type="number">30</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-community-member-link">
                <arguments>
                    <argument name="path" xsi:type="string">community/member</argument>
                    <argument name="label" xsi:type="string" translate="true">Community Profile</argument>
                    <argument name="sortOrder" xsi:type="number">20</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
